<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.UIController">
   <children>
      <HBox prefHeight="100.0" prefWidth="300.0" VBox.vgrow="ALWAYS">
         <children>
            <TextArea id="textView" HBox.hgrow="ALWAYS" />
            <GridPane alignment="CENTER" HBox.hgrow="NEVER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="40.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="40.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" minWidth="40.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="20.0" prefHeight="50.0" vgrow="NEVER" />
                <RowConstraints minHeight="20.0" prefHeight="50.0" vgrow="NEVER" />
                <RowConstraints minHeight="20.0" prefHeight="50.0" vgrow="NEVER" />
              </rowConstraints>
                <children>
                  <Button id="northButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="North" GridPane.columnIndex="1" />
                  <Button id="westButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="West" GridPane.rowIndex="1" />
                  <Button id="eastButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="East" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Button id="southButton" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="South" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                </children>
            </GridPane>
         </children>
      </HBox>
      <HBox prefHeight="20.0" prefWidth="200.0">
            <TextField id="textPrompt" HBox.hgrow="ALWAYS" />
            <Button id="executeButton" mnemonicParsing="false" text="Execute" />
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
